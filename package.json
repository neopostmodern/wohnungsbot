    "flow-typed": "^3.8.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^13.2.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^18.2.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^5.0.1",
    "sass-loader": "^13.3.2",
    "sinon": "^15.2.0",
    "spectron": "^19.0.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^34.0.0",
    "tar": "^6.1.15",
    "terser-webpack-plugin": "^5.3.9",
    "testcafe": "^3.0.1",
    "testcafe-browser-provider-electron": "^0.0.19",
    "testcafe-live": "^0.1.4",
