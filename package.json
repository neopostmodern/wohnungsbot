    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "fbjs-scripts": "^3.0.1",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.178.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^3.7.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.3.0",
    "sinon": "^12.0.1",
    "spectron": "^15.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "tar": "^6.1.11",
    "terser-webpack-plugin": "^5.2.5",
    "testcafe": "^1.17.1",
    "testcafe-browser-provider-electron": "^0.0.16",
    "testcafe-live": "^0.1.4",
    "testcafe-react-selectors": "^4.1.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.2",
