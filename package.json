    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "fbjs-scripts": "^3.0.1",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.212.0",
