    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@sendgrid/mail": "^7.6.0",
    "core-js": "^3.19.1",
    "dot-prop-immutable": "^2.1.1",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.1",
